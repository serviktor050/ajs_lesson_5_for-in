{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "import/extensions":[
            "error","ignorePackages"
        ],
        "no-restricted-syntax": 0,
        "guard-for-in":0,
        "array-callback-return":0,
        "consistent-return":0
    }
}